<GuiXml xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sir.insidi.at/or/schema/esoui.xsd">
    <!-- Fonts -->
	<Font name="MyFontGame16" font="$(MEDIUM_FONT)|$(KB_16)|soft-shadow-thin" />
    <Font name="MyFontGame20" font="$(BOLD_FONT)|$(KB_20)|soft-shadow-thik" />

    <Controls>
<!-- ========================================================================================================================================================== -->
		<!-- Virtual labels -->
        <Label name="FCOGLRowLabel" font="MyFontGame16" wrapMode="ELLIPSIS" horizontalAlignment="LEFT" verticalAlignment="CENTER" virtual="true" tier="LOW" layer="CONTROLS" level="1"/>
		<Label name="FCOGLSearchLabel" font="ZoFontGameLargeBold" color="INTERFACE_COLOR_TYPE_TEXT_COLORS:INTERFACE_TEXT_COLOR_NORMAL" virtual="true" tier="LOW" layer="CONTROLS" level="1"/>

<!-- ========================================================================================================================================================== -->
        <!--Virtual sort headers -->
        <Control name="FCOGLSortHeader" inherits="ZO_SortHeaderBehavior" virtual="true" tier="LOW" layer="CONTROLS" level="1">
			<Controls>
                <Label name="$(parent)Name" font="MyFontGame20" color="INTERFACE_COLOR_TYPE_TEXT_COLORS:INTERFACE_TEXT_COLOR_NORMAL"  horizontalAlignment="CENTER" verticalAlignment="CENTER" wrapMode="ELLIPSIS" tier="LOW" layer="CONTROLS" level="1">
					<AnchorFill />
                </Label>
            </Controls>
        </Control>

<!-- ========================================================================================================================================================== -->
        <!-- Virtual Rows -->
		<Control name="FCOGLRowGuildSales" mouseEnabled="true" virtual="true" tier="LOW" layer="CONTROLS" level="1">
			<Dimensions y="30" />
			<OnMouseEnter>
				FCOGL_UI_OnMouseEnter(self)
			</OnMouseEnter>
			<OnMouseExit>
				FCOGL_UI_OnMouseExit(self)
			</OnMouseExit>
            <OnMouseUp>
				FCOGL_UI_OnMouseUp(self, button, upInside)
			</OnMouseUp>
			<Controls>
				<Texture name="$(parent)BG" inherits="ZO_ThinListBgStrip" tier="LOW" layer="BACKGROUND" level="0"/>
                <Label name="$(parent)Rank" inherits="FCOGLRowLabel">
                    <Anchor point="LEFT" relativePoint="LEFT" relativeTo="$(parent)" />
                    <Dimensions x="50" y="30" />
                </Label>
                <!--
				<Label name="$(parent)DateTime" inherits="FCOGLRowLabel">
                    <Anchor point="LEFT" relativePoint="RIGHT" relativeTo="$(parent)Rank" />
                    <Dimensions x="135" y="30" />
                </Label>
                -->
				<Label name="$(parent)Name" inherits="FCOGLRowLabel">
					<Anchor point="LEFT" relativePoint="RIGHT" relativeTo="$(parent)Rank" />
					<Dimensions x="200" y="30" />
				</Label>
				<!--
				<Label name="$(parent)Item" inherits="FCOGLRowLabel">
					<Anchor point="LEFT" relativePoint="RIGHT" relativeTo="$(parent)Name" />
					<Dimensions x="100" y="30" />
				</Label>
				-->
				<Label name="$(parent)Price" inherits="FCOGLRowLabel">
					<Anchor point="LEFT" relativePoint="RIGHT" relativeTo="$(parent)Name" />
					<Dimensions x="80" y="30" />
				</Label>
				<Label name="$(parent)Tax" inherits="FCOGLRowLabel">
					<Anchor point="LEFT" relativePoint="RIGHT" relativeTo="$(parent)Price" />
					<Dimensions x="80" y="30" />
				</Label>
				<Label name="$(parent)Amount" inherits="FCOGLRowLabel">
					<Anchor point="LEFT" relativePoint="RIGHT" relativeTo="$(parent)Tax" />
					<Dimensions x="150" y="30" />
				</Label>
                <Label name="$(parent)Info" inherits="FCOGLRowLabel">
                    <Anchor point="LEFT" relativePoint="RIGHT" relativeTo="$(parent)Amount" />
                    <Dimensions x="100" y="30" />
					<Anchor point="RIGHT" relativePoint="RIGHT" relativeTo="$(parent)" offsetX="-15" />
                </Label>
			</Controls>
		</Control>

		<Control name="FCOGLRowGuildMembersJoined" mouseEnabled="true" virtual="true" tier="LOW" layer="CONTROLS" level="1">
			<Dimensions y="30" />
			<OnMouseEnter>
				FCOGL_UI_OnMouseEnter(self)
			</OnMouseEnter>
			<OnMouseExit>
				FCOGL_UI_OnMouseExit(self)
			</OnMouseExit>
            <OnMouseUp>
				FCOGL_UI_OnMouseUp(self, button, upInside)
			</OnMouseUp>
			<Controls>
				<Texture name="$(parent)BG" inherits="ZO_ThinListBgStrip" tier="LOW" layer="BACKGROUND" level="0"/>
                <Label name="$(parent)Rank" inherits="FCOGLRowLabel">
                    <Anchor point="LEFT" relativePoint="LEFT" relativeTo="$(parent)" />
                    <Dimensions x="50" y="30" />
                </Label>
                <!--
				<Label name="$(parent)DateTime" inherits="FCOGLRowLabel">
                    <Anchor point="LEFT" relativePoint="RIGHT" relativeTo="$(parent)Rank" />
                    <Dimensions x="135" y="30" />
                </Label>
                -->
				<Label name="$(parent)Name" inherits="FCOGLRowLabel">
					<Anchor point="LEFT" relativePoint="RIGHT" relativeTo="$(parent)Rank" />
					<Dimensions x="200" y="30" />
				</Label>
				<Label name="$(parent)InvitedBy" inherits="FCOGLRowLabel">
					<Anchor point="LEFT" relativePoint="RIGHT" relativeTo="$(parent)Name" />
					<Dimensions x="150" y="30" />
				</Label>
                <Label name="$(parent)Info" inherits="FCOGLRowLabel">
                    <Anchor point="LEFT" relativePoint="RIGHT" relativeTo="$(parent)InvitedBy" />
                    <Dimensions x="100" y="30" />
					<Anchor point="RIGHT" relativePoint="RIGHT" relativeTo="$(parent)" offsetX="-15" />
                </Label>
			</Controls>
		</Control>

		<Control name="FCOGLRowDiceHistory" mouseEnabled="true" virtual="true" tier="LOW" layer="CONTROLS" level="1">
			<Dimensions y="30" />
			<OnMouseEnter>
				FCOGL_UI_OnMouseEnter(self)
			</OnMouseEnter>
			<OnMouseExit>
				FCOGL_UI_OnMouseExit(self)
			</OnMouseExit>
            <OnMouseUp>
				FCOGL_UI_OnMouseUp(self, button, upInside)
			</OnMouseUp>
			<Controls>
				<Texture name="$(parent)BG" inherits="ZO_ThinListBgStrip" tier="LOW" layer="BACKGROUND" level="0" />
                <Label name="$(parent)No" inherits="FCOGLRowLabel">
                    <Anchor point="LEFT" relativePoint="LEFT" relativeTo="$(parent)" />
                    <Dimensions x="30" y="30" />
                </Label>
				<Label name="$(parent)DateTime" inherits="FCOGLRowLabel">
                    <Anchor point="LEFT" relativePoint="RIGHT" relativeTo="$(parent)No" />
                    <Dimensions x="125" y="30" />
                </Label>
				<Label name="$(parent)Name" inherits="FCOGLRowLabel">
					<Anchor point="LEFT" relativePoint="RIGHT" relativeTo="$(parent)DateTime" />
					<Dimensions x="230" y="30" />
					<DimensionConstraints minX="230" minY="30" />
				</Label>
				<Label name="$(parent)Roll" inherits="FCOGLRowLabel">
					<Anchor point="LEFT" relativePoint="RIGHT" relativeTo="$(parent)Name" />
					<Dimensions x="85" y="30" />
					<Anchor point="RIGHT" relativePoint="RIGHT" relativeTo="$(parent)" offsetX="-15" />
				</Label>
			</Controls>
		</Control>

<!-- ========================================================================================================================================================== -->
		<!--Virtual backdrops - The background of the UI -->
		<Backdrop name="FCOGL_WindowsBackdrop" inherits="ZO_DarkThinFrame" virtual="true" tier="LOW" layer="BACKGROUND" level="0">
			<Anchor point="TOPLEFT" offsetX="-4" offsetY="-4"/>
            <Anchor point="BOTTOMRIGHT" offsetX="4" offsetY="4"/>
            <Edge file="EsoUI/Art/Miscellaneous/dark_edgeFrame_8_thin.dds" edgeFileWidth="64" edgeFileHeight="8" />
            <Center file="EsoUI/Art/Miscellaneous/Gamepad/gp_edgeFill.dds" />
			<!--<Center file="/esoui/art/upsell/upsell_greymoor_bg.dds" /> -->
        </Backdrop>

		<!-- Virtual ComboBox backdrop -->
		<Backdrop name="FCOGL_MultiSeletComboBoxBG" inherits="ZO_DarkThinFrame" virtual="true" tier="LOW" layer="CONTROLS" level="1">
			<AnchorFill />
			<Edge file="EsoUI/Art/Miscellaneous/dark_edgeFrame_8_thin.dds" edgeFileWidth="64" edgeFileHeight="8" />
			<Center file="EsoUI/Art/Miscellaneous/Gamepad/gp_edgeFill.dds" />
			<!--<Center file="/esoui/art/upsell/upsell_greymoor_bg.dds" /> -->
		</Backdrop>

<!-- ========================================================================================================================================================== -->
		<!-- Virtual comboboxes -->
		<Control name="FCOGLMultiselectComboBox" mouseEnabled="true" inherits="ZO_MultiselectComboBox" virtual="true" tier="LOW" layer="CONTROLS" level="1">
			<Dimensions x="230" y="28" />
			<Controls>
				<Backdrop name="$(parent)BGNew" inherits="FCOGL_MultiSeletComboBoxBG">
					<AnchorFill />
				</Backdrop>
			</Controls>
        </Control>


<!-- ========================================================================================================================================================== -->
		<!-- Virtual edit boxes -->
		<EditBox name="FCOGLEditForBackdrop" inherits="ZO_DefaultEdit" virtual="true" tier="LOW" layer="CONTROLS" level="1">
            <Anchor point="TOPLEFT" offsetX="2" offsetY="2"/>
            <Anchor point="BOTTOMRIGHT" offsetX="-2" offsetY="-2"/>
        </EditBox>
		<EditBox name="FCOGLEditDefaultText" virtual="true" tier="LOW" layer="CONTROLS" level="1">
            <OnTextChanged>
                ZO_EditDefaultText_OnTextChanged(self)
            </OnTextChanged>
            <OnIMEBeginComposition>
                ZO_EditDefaultText_OnIMECompositionChanged(self)
            </OnIMEBeginComposition>
            <OnIMEEndComposition>
                ZO_EditDefaultText_OnIMECompositionChanged(self)
            </OnIMEEndComposition>
            <Controls>
                <Label name="$(parent)Text" color="INTERFACE_COLOR_TYPE_TEXT_COLORS:INTERFACE_TEXT_COLOR_DEFAULT_TEXT" font="MyFontGame16" tier="LOW" layer="CONTROLS" level="1">
                    <Anchor point="TOPLEFT" offsetX="1"/>
                    <Anchor point="BOTTOMRIGHT"/>
                </Label>
            </Controls>
        </EditBox>

<!-- ========================================================================================================================================================== -->
		<!-- Virtual Buttons -->
		<Button name="FCOGLButtonGuildSales" virtual="true" tier="LOW" layer="CONTROLS" level="1">
			<Dimensions x="50" y="50" />
			<Textures normal="/esoui/art/tradinghouse/tradinghouse_sell_tabicon_up.dds"
					  mouseOver="/esoui/art/tradinghouse/tradinghouse_sell_tabicon_up.dds"
					  pressed="/esoui/art/tradinghouse/tradinghouse_sell_tabicon_up.dds"
					  disabled="/esoui/art/tradinghouse/tradinghouse_sell_tabicon_disabled.dds"
			/>
			<OnClicked>
				FCOGL.UI.SetTab(FCOGL_TAB_GUILDSALESLOTTERY, false)
			</OnClicked>
			<OnMouseUp>
				if button == MOUSE_BUTTON_INDEX_RIGHT and upInside then
				   FCOGL.UI.showTabButtonContextMenu(self)
				end
			</OnMouseUp>
			<OnMouseEnter>
				FCOGL.ShowTooltip(self, TOP, GetString(FCOGL_GUILD_SALES_LOTTERY))
			</OnMouseEnter>
			<OnMouseExit>
				FCOGL.HideTooltip()
			</OnMouseExit>
		</Button>
		<Button name="FCOGLButtonList" virtual="true" tier="LOW" layer="CONTROLS" level="1">
			<Dimensions x="50" y="50" />
			<Textures normal="/esoui/art/journal/journal_tabicon_cadwell_up.dds"
					  mouseOver="/esoui/art/journal/journal_tabicon_cadwell_over.dds"
					  pressed="/esoui/art/journal/journal_tabicon_cadwell_down.dds" />
			<OnClicked>
				FCOGL.UI.SetTab(FCOGL_TAB_FCOGL)
			</OnClicked>
			<OnMouseEnter>
				FCOGL.ShowTooltip(self, TOP, "List")
			</OnMouseEnter>
			<OnMouseExit>
				FCOGL.HideTooltip()
			</OnMouseExit>
		</Button>
		<Button name="FCOGLButtonDiceHistory" virtual="true" tier="LOW" layer="CONTROLS" level="1">
			<Dimensions x="50" y="50" />
			<Textures normal="/esoui/art/guild/tabicon_history_up.dds"
					  mouseOver="/esoui/art/guild/tabicon_history_over.dds"
					  pressed="/esoui/art/guild/tabicon_history_down.dds"
					  disabled="/esoui/art/guild/tabicon_history_disabled.dds"
			/>
			<OnClicked>
				FCOGL.UI.ToggleDiceRollHistory()
			</OnClicked>
			<OnMouseEnter>
				FCOGL.ShowTooltip(self, TOP, GetString(FCOGL_TOGGLE_DICE_ROLL_HISTORY))
			</OnMouseEnter>
			<OnMouseExit>
				FCOGL.HideTooltip()
			</OnMouseExit>
		</Button>
		<Button name="FCOGLButtonSettings" virtual="true" tier="LOW" layer="CONTROLS" level="1">
			<Dimensions x="28" y="28" />
			<Textures normal="/esoui/art/chatwindow/chat_options_up.dds"
					  pressed="/esoui/art/chatwindow/chat_options_down.dds"
					  mouseOver="/esoui/art/chatwindow/chat_options_over.dds"
					  disabled="/esoui/art/chatwindow/chat_options_up.dds"/>
			<OnClicked>
				FCOGL.ShowLAMSettings()
			</OnClicked>
			<OnMouseEnter>
				FCOGL.ShowTooltip(self, TOP, GetString(FCOGL_SETTINGS))
			</OnMouseEnter>
			<OnMouseExit>
				FCOGL.HideTooltip()
			</OnMouseExit>
		</Button>
		<Button name="FCOGLButtonClose" virtual="true" tier="LOW" layer="CONTROLS" level="1">
			<Dimensions x="36" y="36" />
			<Textures normal="/esoui/art/buttons/closebutton_up.dds"
					  pressed="/esoui/art/buttons/closebutton_down.dds"
					  mouseOver="/esoui/art/buttons/closebutton_mouseOver.dds"
					  disabled="/esoui/art/buttons/closebutton_disabled.dds" />
			<OnClicked>
				FCOGL.UI.Show(false)
			</OnClicked>
			<OnMouseEnter>
				FCOGL.ShowTooltip(self, TOP, GetString(FCOGL_CLOSE))
			</OnMouseEnter>
			<OnMouseExit>
				FCOGL.HideTooltip()
			</OnMouseExit>
		</Button>
		<Button name="FCOGLButtonReload" virtual="true" tier="LOW" layer="CONTROLS" level="1">
			<Dimensions x="24" y="24" />
			<Textures normal="/esoui/art/help/help_tabicon_feedback_up.dds"
					  mouseOver="/esoui/art/help/help_tabicon_feedback_over.dds"
                      pressed="/esoui/art/help/help_tabicon_feedback_down.dds"
                      disabled="/esoui/art/hud/gamepad/gp_radialicon_trade_disabled.dds" />
			<OnClicked>
				FCOGL.ReloadGuildSalesLotteryRanks()
			</OnClicked>
			<OnMouseEnter>
				FCOGL.ShowTooltip(self, RIGHT, GetString(FCOGL_REFRESH))
			</OnMouseEnter>
			<OnMouseExit>
				FCOGL.HideTooltip()
			</OnMouseExit>
		</Button>
		<Button name="FCOGLButtonStop" virtual="true" tier="LOW" layer="CONTROLS" level="1">
			<Dimensions x="24" y="24" />
			<Textures normal="/esoui/art/buttons/cancel_up.dds"
					  mouseOver="/esoui/art/buttons/cancel_over.dds"
                      pressed="/esoui/art/buttons/cancel_down.dds"
                      disabled="/esoui/art/buttons/cancel_disabled.dds" />
			<OnClicked>
				FCOGL.StopGuildSalesLottery()
			</OnClicked>
			<OnMouseEnter>
				FCOGL.ShowTooltip(self, RIGHT, GetString(FCOGL_STOP_GUILDSALESLOTTERY))
			</OnMouseEnter>
			<OnMouseExit>
				FCOGL.HideTooltip()
			</OnMouseExit>
		</Button>
		<Button name="FCOGLButtonNewGuildSalesLottery" virtual="true" tier="LOW" layer="CONTROLS" level="1">
			<Dimensions x="30" y="30" />
			<Textures normal="/esoui/art/buttons/large_rightarrow_up.dds"
					  mouseOver="/esoui/art/buttons/large_rightarrow_over.dds"
                      pressed="/esoui/art/buttons/large_rightarrow_down.dds"
					  disabled="/esoui/art/buttons/large_rightarrow_disabled.dds"/>
			<OnClicked>
				FCOGL.CheckBeforeStartNew(FCOGLFrameGuildsDrop.m_comboBox.m_selectedItemData.index, FCOGL.GetGuildSalesLotteryStartDate(), false, FCOGL_DICE_ROLL_TYPE_GUILD_SALES_LOTTERY)
			</OnClicked>
			<OnMouseEnter>
				FCOGL.ShowTooltip(self, RIGHT, GetString(FCOGL_START_NEW_GUILD_SALES_LOTTERY))
			</OnMouseEnter>
			<OnMouseExit>
				FCOGL.HideTooltip()
			</OnMouseExit>
		</Button>
		<Button name="FCOGLButtonStopGuildMemberJoinedList" virtual="true" tier="LOW" layer="CONTROLS" level="1">
			<Dimensions x="24" y="24" />
			<Textures normal="/esoui/art/buttons/cancel_up.dds"
					  mouseOver="/esoui/art/buttons/cancel_over.dds"
                      pressed="/esoui/art/buttons/cancel_down.dds"
                      disabled="/esoui/art/buttons/cancel_disabled.dds" />
			<OnClicked>
				FCOGL.StopGuildMembersJoinDate()
			</OnClicked>
			<OnMouseEnter>
				FCOGL.ShowTooltip(self, RIGHT, GetString(FCOGL_STOP_GUILD_MEMBER_JOINED_LIST))
			</OnMouseEnter>
			<OnMouseExit>
				FCOGL.HideTooltip()
			</OnMouseExit>
		</Button>
		<Button name="FCOGLButtonNewGuildMemberJoinedList" virtual="true" tier="LOW" layer="CONTROLS" level="1">
			<Dimensions x="34" y="34" />
			<Textures normal="/esoui/art/icons/guildranks/guild_indexicon_member_up.dds"
					  mouseOver="/esoui/art/icons/guildranks/guild_indexicon_member_over.dds"
                      pressed="/esoui/art/icons/guildranks/guild_indexicon_member_down.dds"
					  disabled="/esoui/art/icons/guildranks/guild_indexicon_member_disabled.dds"/>
			<OnClicked>
				FCOGL.CheckBeforeStartNew(FCOGLFrameGuildsDrop.m_comboBox.m_selectedItemData.index, FCOGL.GetGuildMemberJoinedDateStartDate(), false, FCOGL_DICE_ROLL_TYPE_GUILD_MEMBERS_JOIN_DATE)
			</OnClicked>
			<OnMouseEnter>
				FCOGL.ShowTooltip(self, RIGHT, GetString(FCOGL_START_NEW_GUILD_MEMBER_JOINED_LIST))
			</OnMouseEnter>
			<OnMouseExit>
				FCOGL.HideTooltip()
			</OnMouseExit>
		</Button>
		<Button name="FCOGLButtonRollTheDice" virtual="true" tier="LOW" layer="CONTROLS" level="1">
			<Dimensions x="30" y="30" />
			<Textures normal="/esoui/art/lfg/gamepad/gp_lfg_menuicon_random.dds"
					  mouseOver="/esoui/art/lfg/gamepad/gp_lfg_menuicon_random.dds"
                      pressed="/esoui/art/guild/guildheraldry_indexicon_background_disabled.dds"
					  disabled = "/esoui/art/loot/dice_disabled.dds" />
			<OnClicked>
				FCOGL.RollTheDiceCheck()
			</OnClicked>
			<OnMouseEnter>
				FCOGL.ShowTooltip(self, RIGHT, GetString(FCOGL_ROLL_THE_DICE))
			</OnMouseEnter>
			<OnMouseExit>
				FCOGL.HideTooltip()
			</OnMouseExit>
		</Button>

		<Button name="FCOGLButtonClearHistory" virtual="true" tier="LOW" layer="CONTROLS" level="1">
			<Dimensions x="24" y="24" />
			<Textures normal="/esoui/art/buttons/pointsminus_up.dds"
					  mouseOver="/esoui/art/buttons/pointsminus_over.dds"
                      pressed="/esoui/art/buttons/pointsminus_down.dds"
                      disabled="/esoui/art/buttons/pointsminus_disabled.dds" />
			<OnClicked>
				FCOGL.ClearCurrentHistoryCheck()
			</OnClicked>
			<OnMouseEnter>
				FCOGL.ShowTooltip(self, RIGHT, GetString(FCOGL_CLEAR_HISTORY))
			</OnMouseEnter>
			<OnMouseExit>
				FCOGL.HideTooltip()
			</OnMouseExit>
		</Button>

		<Button name="FCOGLButtonDeleteSelectedHistory" virtual="true" tier="LOW" layer="CONTROLS" level="1">
			<Dimensions x="24" y="24" />
			<Textures normal="/esoui/art/inventory/inventory_tabicon_junk_up.dds"
					  mouseOver="/esoui/art/inventory/inventory_tabicon_junk_over.dds"
                      pressed="/esoui/art/inventory/inventory_tabicon_junk_down.dds"
                      disabled="/esoui/art/inventory/inventory_tabicon_junk_disabled.dds" />
			<OnClicked>
				FCOGL.UI.checkDeleteSelectedGuildSalesLotteryHistoryEntries()
			</OnClicked>
			<OnMouseEnter>
				FCOGL.ShowTooltip(self, RIGHT, GetString(FCOGL_GUILDSALESLOTTERY_DELETE_SELECTED))
			</OnMouseEnter>
			<OnMouseExit>
				FCOGL.HideTooltip()
			</OnMouseExit>
		</Button>

<!-- ========================================================================================================================================================== -->
<!-- ========================================================================================================================================================== -->
<!-- ========================================================================================================================================================== -->

		<!-- Frame/Window - Standard UI with border -->
		<TopLevelControl name="FCOGLFrame" mouseEnabled="true" movable="true" clampedToScreen="true" hidden="true" tier="LOW" layer="CONTROLS" level="1"> <!--inherits="ZO_RightPanelFootPrint"-->
			<Dimensions x="730" y="690" />
			<DimensionConstraints minX="400" minY="300" />
			<Anchor point="CENTER" relativeTo="GuiRoot" relativePoint="CENTER" offsetX="300" offsetY="-100"/>
			<OnMoveStop>
				FCOGL.UI.OnWindowMoveStop()
			</OnMoveStop>
			<Controls>
				<Backdrop name="$(parent)Search" inherits="ZO_SingleLineEditBackdrop_Keyboard" tier="HIGH" layer="CONTROLS" level="1">
					<Anchor point="TOPRIGHT" relativePoint="TOPRIGHT" offsetX="-8" offsetY="14" />
					<Dimensions x="200" />
					<Controls>
						<EditBox name="$(parent)Box" inherits="ZO_DefaultEditForBackdrop ZO_EditDefaultText"/>
					</Controls>
				</Backdrop>

				<Control name="$(parent)SearchDrop" resizeToFitDescendents="true" mouseEnabled="true" tier="LOW" layer="CONTROLS" level="1">
					<Anchor point="RIGHT" relativePoint="LEFT" relativeTo="$(parent)Search" />
					<OnInitialized>
						ZO_ComboBox:New(self)
					</OnInitialized>
					<OnMouseUp>
						if button == MOUSE_BUTTON_INDEX_LEFT and upInside then
							ZO_ComboBox_DropdownClicked(self)
						end
					</OnMouseUp>
					<Controls>
						<Label name="$(parent)Anchor" inherits="FCOGLSearchLabel">
							<Anchor point="RIGHT" />
						</Label>
						<Label name="$(parent)ColonText" text=":" inherits="FCOGLSearchLabel">
							<Anchor point="RIGHT" relativeTo="$(parent)Anchor" relativePoint="LEFT" offsetX="-8" />
						</Label>
						<Label name="$(parent)SelectedItemText" inherits="FCOGLSearchLabel">
							<Anchor point="RIGHT" relativeTo="$(parent)ColonText" relativePoint="LEFT" offsetX="-1" />
						</Label>
						<Button name="$(parent)OpenDropdown" inherits="ZO_DropdownButton">
							<Dimensions x="16" y="16" />
							<Anchor point="RIGHT" relativeTo="$(parent)SelectedItemText" relativePoint="LEFT" offsetX="-3" />
							<OnClicked>
								ZO_ComboBox_DropdownClicked(self:GetParent())
							</OnClicked>
						</Button>
					</Controls>
				</Control>
				<Control name="$(parent)GuildsDrop" resizeToFitDescendents="true" mouseEnabled="true" tier="LOW" layer="CONTROLS" level="1">
					<Anchor point="RIGHT" relativePoint="LEFT" relativeTo="$(parent)SearchDrop" offsetX="-10" />
					<OnInitialized>
						ZO_ComboBox:New(self)
					</OnInitialized>
					<OnMouseUp>
							if (button == MOUSE_BUTTON_INDEX_LEFT and upInside) then
								ZO_ComboBox_DropdownClicked(self)
							end
					</OnMouseUp>
                    <Controls>
                        <Label name="$(parent)Anchor" inherits="FCOGLSearchLabel">
                            <Anchor point="RIGHT" />
                        </Label>
                        <Label name="$(parent)ColonText" text="" inherits="FCOGLSearchLabel">
                            <Anchor point="RIGHT" relativeTo="$(parent)Anchor" relativePoint="LEFT" offsetX="-8" />
                        </Label>
                        <Label name="$(parent)SelectedItemText" inherits="FCOGLSearchLabel">
                            <Anchor point="RIGHT" relativeTo="$(parent)ColonText" relativePoint="LEFT" offsetX="-1" />
                        </Label>
                        <Button name="$(parent)OpenDropdown" inherits="ZO_DropdownButton">
                            <Dimensions x="16" y="16" />
                            <Anchor point="RIGHT" relativeTo="$(parent)SelectedItemText" relativePoint="LEFT" offsetX="-3" />
